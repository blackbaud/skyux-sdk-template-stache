{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "app": {
    "title": "Stache Template"
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "url": "your-repo-url"
      },      
      "searchConfig": {
        "allowSiteToBeSearched": false
      }
    }
  },
  "compileMode": "aot",
  "mode": "easy",
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache",
    "@blackbaud/skyux-builder-plugin-code-block"
  ]
}
